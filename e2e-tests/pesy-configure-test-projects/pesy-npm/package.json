{
  "name": "pesy-npm",
  "version": "0.0.0",
  "description": "My Project",
  "esy": {
    "build": "dune build -p #{self.name}",
    "release": {
      "releasedBinaries": [
        "PesyNpmApp.exe"
      ]
    }
  },
  "buildDirs": {
    "test": {
      "require": ["pesy-npm/library"],
      "bin": "TestPesyNpm.re"
    },
    "library": {},
    "executable": {
      "require": ["pesy-npm/library"],
      "main": "PesyNpmApp.re"
    }
  },
  "scripts": {
    "test": "esy x TestPesyNpm.exe"
  },
  "dependencies": {
    "@opam/dune": ">=1.6.0",
    "@esy-ocaml/reason": "*",
    "refmterr": "*",
    "ocaml": "^4.4.0",
    "pesy": "*"
  },
  "devDependencies": {
    "@opam/merlin": "*"
  }
}
