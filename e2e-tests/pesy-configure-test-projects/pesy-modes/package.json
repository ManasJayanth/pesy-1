{
  "name": "pesy-mode",
  "version": "0.0.0",
  "description": "My Project",
  "esy": {
    "build": "dune build -p #{self.name}",
    "buildsInSource": "_build",
    "release": {
      "releasedBinaries": [
        "PesyModeApp.exe"
      ]
    }
  },
  "buildDirs": {
    "test": {
      "require": [
        "pesy-mode.lib"
      ],
      "bin": "TestPesyMode.re",
      "modes": [
        "byte",
        "exe"
      ]
    },
    "library": {
      "name": "pesy-mode.lib",
      "namespace": "PesyMode",
      "modes": [
        "byte"
      ]
    },
    "executable": {
      "require": [
        "pesy-mode.lib"
      ],
      "bin": {
        "PesyModeApp.exe": "PesyModeApp.re"
      },
      "modes": [
        "byte",
        "exe"
      ]
    }
  },
  "scripts": {
    "pesy": "bash -c 'env PESY_MODE=update pesy'",
    "test": "esy b dune runtest"
  },
  "dependencies": {
    "@opam/dune": ">=1.6.0",
    "@esy-ocaml/reason": "*",
    "refmterr": "*",
    "pesy": "*",
    "ocaml": "^4.4.0"
  },
  "devDependencies": {
    "@opam/merlin": "*"
  },
  "resolutions": {
    "pesy": "esy/pesy#595f01ccc2ff251c1860063014bc6a6924a64a19"
  }
}
