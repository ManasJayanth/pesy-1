{
  "name": "test-project",
  "version": "0.0.0",
  "description": "My Project",
  "esy": {
    "build": "dune build -p #{self.name}",
    "release": { "releasedBinaries": [ "TestProjectApp.exe" ] }
  },
  "buildDirs": {
    "test": {
      "require": [ "test-project/library"],
      "bin": "TestTestProject.re as TestTestProject.exe"
    },
    "library": {},
    "executable": {
      "require": [ "test-project/library", "lwt", "lwt/unix" ],
      "bin": "TestProjectApp.re as TestProjectApp.exe",
      "preprocess": [ "pps", "lwt_ppx" ]
    }
  },
  "scripts": { "test": "esy x TestTestProject.exe" },
  "dependencies": {
    "@esy-ocaml/reason": "*",
    "@opam/dune": "*",
    "@opam/lwt": "4.1.0",
    "@opam/lwt_ppx": "1.2.1",
    "ocaml": "~4.6.0",
    "refmterr": "*"
  },
  "devDependencies": { "@opam/merlin": "*" }
}
