{
  "name": "pesy-c-stubs",
  "version": "0.0.0",
  "description": "My Project",
  "esy": {
    "build": "dune build -p #{self.name}",
    "release": {
      "releasedBinaries": [
        "PesyCStubsApp.exe"
      ]
    }
  },
  "buildDirs": {
    "test": {
      "require": ["pesy-c-stubs.lib"],
      "bin": "TestPesyCStubs.re"
    },
    "library": {
      "name": "pesy-c-stubs.lib",
      "cNames": ["stubs"],
      "namespace": "PesyCStubs"
    },
    "executable": {
      "require": ["pesy-c-stubs.lib"],
      "bin": {
        "PesyCStubsApp.exe": "PesyCStubsApp.re"
      }
    }
  },
  "scripts": {
    "test": "esy b dune runtest"
  },
  "dependencies": {
    "@opam/dune": ">=1.6.0",
    "@esy-ocaml/reason": "*",
    "refmterr": "*",
    "ocaml": "^4.4.0"
  },
  "devDependencies": {
    "@opam/merlin": "*"
  }
}
