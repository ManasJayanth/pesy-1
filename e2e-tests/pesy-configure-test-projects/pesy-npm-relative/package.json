{
  "name": "pesy-npm",
  "version": "0.0.0",
  "description": "My Project",
  "esy": {
    "build": "dune build -p #{self.name}",
    "release": {
      "releasedBinaries": [
        "PesyNpmApp.exe"
      ]
    }
  },
  "buildDirs": {
    "test": {
      "require": ["../library"],
      "bin": "TestPesyNpm.re as test-pesy-npm.exe"
    },
    "library": {},
    "library/foo/bar": {
      "require": ["../../"]
    },
    "executable": {
      "require": ["../library/foo/bar", "../library"],
      "bin": "PesyNpmApp.re as pesy-npm-app.exe"
    }
  },
  "scripts": {
    "test": "esy x test-pesy-npm.exe"
  },
  "dependencies": {
    "@opam/dune": ">=1.6.0",
    "@esy-ocaml/reason": "*",
    "refmterr": "*",
    "ocaml": "^4.4.0",
    "pesy": "*"
  },
  "devDependencies": {
    "@opam/merlin": "*"
  }
}
